
def shared_pods
    pod 'CloudCore', :path => '../CloudCore'
end

target 'CarShuffle' do
    platform :ios, '15.0'
    use_frameworks!
    
    shared_pods
    
    pod 'Connectivity'
    pod 'WhatsNewKit'
    
    pod 'Eureka'
    pod 'SwiftDate', :path => '../SwiftDate'
end

